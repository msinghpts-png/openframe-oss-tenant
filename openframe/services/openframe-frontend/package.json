{
  "name": "openframe-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-4000}",
    "dev:auth": "NEXT_PUBLIC_APP_MODE=auth-only next dev -p ${PORT:-4000}",
    "dev:full": "NEXT_PUBLIC_APP_MODE=full-app next dev -p ${PORT:-4000}",
    "build": "next build",
    "build:auth": "NEXT_PUBLIC_APP_MODE=auth-only next build",
    "build:full": "NEXT_PUBLIC_APP_MODE=full-app next build",
    "start": "next start -p ${PORT:-4000}",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@flamingo/ui-kit": "file:./ui-kit",
    "@tanstack/react-query": "^5.80.6",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "highlight.js": "^11.11.1",
    "immer": "^10.1.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.4.0-canary.51",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
}
