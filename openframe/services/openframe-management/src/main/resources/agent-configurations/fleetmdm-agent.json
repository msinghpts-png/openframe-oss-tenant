{
  "id": "fleetmdm-agent",
  "toolId": "fleetmdm-server",
  "version": "0.0.2",
  "status": "ENABLED",
  "sessionType": "SERVICE",
  "runCommandArgs": [
    "--fleet-url", "${client.serverUrl}",
    "--enroll-secret", "${server.registrationSecret}",
    "--insecure",
    "--disable-updates",
    "--openframe-mode",
    "--openframe-secret", "${client.openframeSecret}",
    "--openframe-osquery-path", "${client.assetPath.osqueryd}",
    "--openframe-token-path", "${client.openframeTokenPath}"
  ],
  "agentToolIdCommandArgs": [
    "uuid",
    "--openframe-mode",
    "--openframe-osquery-path", "${client.assetPath.osqueryd}"
  ],
  "downloadConfigurations": [
    {
      "os": "windows",
      "linkTemplate": "https://github.com/flamingo-stack/fleet/releases/download/{version}/fleet-windows-amd64.zip",
      "fileName": "fleet-windows-amd64.zip",
      "agentFileName": "fleet.exe"
    },
    {
      "os": "macos",
      "linkTemplate": "https://github.com/flamingo-stack/fleet/releases/download/{version}/fleet-macos-universal.tar.gz",
      "fileName": "fleet-macos-universal.tar.gz",
      "agentFileName": "fleet"
    }
  ],
  "assets": [
    {
      "id": "osqueryd",
      "localFilename": "osqueryd",
      "source": "ARTIFACTORY",
      "executable": true
    }
  ]
}
