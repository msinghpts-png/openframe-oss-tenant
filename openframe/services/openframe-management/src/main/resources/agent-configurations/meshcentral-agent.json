{
  "id": "meshcentral-agent",
  "toolId": "meshcentral-server",
  "version": "0.0.3",
  "status": "ENABLED",
  "sessionType": "CONSOLE",
  "installationCommandArgs": [
    "-install",
    "--openframe-mode",
    "--openframe-secret",
    "${client.openframeSecret}",
    "--openframe-token-path",
    "${client.openframeTokenPath}"
  ],
  "runCommandArgs": [],
  "uninstallationCommandArgs": [
    "-uninstall"
  ],
  "agentToolIdCommandArgs": [
    "-nodeid-base64"
  ],
  "downloadConfigurations": [
    {
      "os": "windows",
      "linkTemplate": "https://github.com/flamingo-stack/MeshAgent/releases/download/{version}/meshagent-windows-amd64.zip",
      "fileName": "meshagent-windows-amd64.zip",
      "agentFileName": "meshagent.exe"
    },
    {
      "os": "macos",
      "linkTemplate": "https://github.com/flamingo-stack/MeshAgent/releases/download/{version}/meshagent-macos-universal.tar.gz",
      "fileName": "meshagent-macos-universal.tar.gz",
      "agentFileName": "meshagent"
    }
  ],
  "assets": [
    {
      "id": "meshcentral-core-module",
      "localFilename": "CoreModule.js",
      "source": "ARTIFACTORY",
      "executable": false
    },
    {
      "id": "msh-configuration",
      "localFilename": "agent.msh",
      "source": "TOOL_API",
      "path": "/generate-msh?host=${client.serverUrl}",
      "executable": false
    }
  ]
}
