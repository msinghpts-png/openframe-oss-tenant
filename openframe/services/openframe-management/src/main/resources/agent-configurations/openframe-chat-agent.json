{
  "id": "openframe-chat",
  "version": "0.0.16",
  "status": "ENABLED",
  "sessionType": "USER",
  "runCommandArgs": [
    "--serverUrl", "${client.serverUrl}",
    "--openframe-secret", "${client.openframeSecret}",
    "--openframe-token-path", "${client.openframeTokenPath}"
  ],
  "agentToolIdCommandArgs": [],
  "downloadConfigurations": [
    {
      "os": "windows",
      "linkTemplate": "https://github.com/flamingo-stack/openframe-oss-tenant/releases/download/{version}/openframe-chat_windows.zip",
      "fileName": "openframe-chat_windows.zip",
      "agentFileName": "openframe-chat.exe"
    },
    {
      "os": "macos",
      "linkTemplate": "https://github.com/flamingo-stack/openframe-oss-tenant/releases/download/{version}/openframe-chat_macos.tar.gz",
      "fileName": "openframe-chat_macos.tar.gz",
      "agentFileName": "openframe-chat"
    }
  ]
}

