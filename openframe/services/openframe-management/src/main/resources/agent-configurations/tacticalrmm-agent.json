{
  "id": "tacticalrmm-agent",
  "toolId": "tactical-rmm",
  "version": "0.0.2",
  "status": "ENABLED",
  "sessionType": "SERVICE",
  "installationCommandArgs": [
    "-m", "install",
    "-api", "${client.serverUrl}",
    "-auth", "${server.registrationSecret}",
    "-client-id", "1",
    "-site-id", "1",
    "-agent-type", "workstation",
    "-log", "INFO",
    "-logto", "stdout",
    "--openframe-mode",
    "-nomesh",
    "--openframe-secret", "${client.openframeSecret}",
    "--openframe-token-path", "${client.openframeTokenPath}",
    "--insecure",
    "--silent"
  ],
  "uninstallationCommandArgs": [
    "-m", "cleanup",
    "--openframe-mode",
    "--openframe-secret", "${client.openframeSecret}",
    "--openframe-token-path", "${client.openframeTokenPath}"
  ],
  "runCommandArgs": [
    "-m", "svc",
    "-log", "INFO",
    "-logto", "stdout",
    "-openframe-secret", "${client.openframeSecret}",
    "--openframe-token-path", "${client.openframeTokenPath}"
  ],
  "agentToolIdCommandArgs": [
    "-m", "agentid",
    "--openframe-mode",
    "-openframe-secret", "${client.openframeSecret}",
    "--openframe-token-path", "${client.openframeTokenPath}"
  ],
  "downloadConfigurations": [
    {
      "os": "windows",
      "linkTemplate": "https://github.com/flamingo-stack/rmmagent/releases/download/{version}/rmmagent-windows-amd64.zip",
      "fileName": "rmmagent-windows-amd64.zip",
      "agentFileName": "rmmagent.exe"
    },
    {
      "os": "macos",
      "linkTemplate": "https://github.com/flamingo-stack/rmmagent/releases/download/{version}/rmmagent-macos-universal.tar.gz",
      "fileName": "rmmagent-macos-universal.tar.gz",
      "agentFileName": "rmmagent"
    }
  ]
}
